}

switch(FEAT_TRAINS, SELF, switch_jgr_8620_purchase, current_year) {
	0..1949: spriteset_jgr_8620_jgr_purchase;
	spriteset_jgr_8620_jnr_purchase;
	}

item (FEAT_TRAINS, jgr_8620, 944) {
	property {
		sprite_id: SPRITE_ID_NEW_TRAIN;
		track_type: TR_1067_NE;
		name: string(STR_NAME_8620);
		climates_available: ALL_CLIMATES;
		introduction_date: date(1914,1,1);
		speed: 95 km/h;
		air_drag_coefficient: 0;
		tractive_effort_coefficient:    0.26;
		power: 567 kW;
		dual_headed: 0;
		engine_class: ENGINE_CLASS_STEAM;
		cargo_capacity: 1;
		cargo_allow_refit: [PASS];
		weight: 83;
		reliability_decay: 20;
		model_life: 40;
		vehicle_life: 60;
		cost_factor: 25;
		length: 6;
		running_cost_factor: 35;
		running_cost_base: RUNNING_COST_STEAM;
		misc_flags: bitmask(TRAIN_FLAG_2CC);
	}
	graphics { // graphics for engine
		additional_text:	string(STR_CONCAT_5S, 
								string(STR_DESC_POWER_STEAM),
								string(STR_DESC_WHEELS_1C),
								string(STR_DESC_NARROWGAUGE),
								string(STR_DESC_USAGE_EP),
								string(STR_CONCAT_3C, 
									string(STR_SUFFIX_JGR),
									string(STR_SUFFIX_JNR),
									string(STR_SUFFIX_PRIVATE)

                    )
                  );
		cargo_capacity: return 0;
		cargo_subtype_text: switch_jgr_8620_subtype_text;
		default: switch_jgr_8620_livery;
		purchase: switch_jgr_8620_purchase;
		articulated_part: switch_jgr_8620_articulated_part;
	}
}


