//refit

random_switch(FEAT_TRAINS, SELF, switch_jgr_8620_random) {
	1: spriteset_jgr_8620_nosd_front;
	1: spriteset_jgr_8620_smallsd_front;
	1: spriteset_jgr_8620_bigsd_front;
	}


switch(FEAT_TRAINS, SELF, switch_jgr_8620_livery, cargo_subtype) {
	1: spriteset_jgr_8620_nosd_front;
	2: spriteset_jgr_8620_smallsd_front;
	3: spriteset_jgr_8620_bigsd_front;
	4: spriteset_jgr_8620_gold_front;
	5: spriteset_jgr_8620_cc_front;
	switch_jgr_8620_random;
	
}




switch(FEAT_TRAINS, SELF, switch_jgr_8620_subtype_text, cargo_subtype) {
	0: return string(STR_REFIT_RANDOM);
	1: return string(STR_CONCAT_3, string(STR_SUFFIX_JGR), 
                                    string(STR_CONCAT_2F, 
                                     string(STR_COLOUR_BLACK),
									 string(STR_COLOUR_WHITE)
                                    )
                                 );
	2: return string(STR_CONCAT_3, string(STR_SUFFIX_JGR), 
                                    string(STR_CONCAT_2F, 
                                     string(STR_COLOUR_BLACK),
									 string(STR_COLOUR_WHITE)
                                    ), string(STR_SUFFIX_SMALLDEF)
                                 );
	3: return string(STR_CONCAT_3, string(STR_SUFFIX_JGR), 
                                    string(STR_CONCAT_2F, 
                                     string(STR_COLOUR_BLACK),
									 string(STR_COLOUR_WHITE)
                                    ), string(STR_SUFFIX_SMOKEDEF)
                                 );
	4: return string(STR_CONCAT_3, string(STR_SUFFIX_JGR), 
                                    string(STR_CONCAT_2F, 
                                     string(STR_COLOUR_BLACK),
									 string(STR_COLOUR_GOLD)
                                    ), string(STR_SUFFIX_SMALLDEF)
                                 );
	5: return string(STR_REFIT_CC);		
	   return CB_RESULT_NO_TEXT;
	}




