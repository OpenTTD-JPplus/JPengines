  1: return CB_RESULT_NO_TEXT; 
  return string(STR_NAME_EF59);
  } 
switch (FEAT_TRAINS, SELF, switch_ef59_2_name, getbits(extra_callback_info1, 0, 8)) { 
  0x20: switch_ef59_2_name_purchase; 
  return CB_RESULT_NO_TEXT;
  }


