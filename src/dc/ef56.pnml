		power: 1350 kW;
		dual_headed: 0;
		engine_class: ENGINE_CLASS_ELECTRIC;
		cargo_capacity: 1;
		cargo_allow_refit: [PASS];
		weight: 110;
		reliability_decay: 20;
		model_life: 45;
		vehicle_life: 45;
		cost_factor: 42;
		length: 8;
		running_cost_factor: 69;
		running_cost_base: RUNNING_COST_ELECTRIC;
		misc_flags: bitmask(TRAIN_FLAG_SPRITE_STACK);
	}
	graphics {
